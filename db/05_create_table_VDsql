CREATE TABLE SHM_SEG_USUARIO_SEDE(
  ID_USUARIO Number NOT NULL,
  ID_SEDE Number NOT NULL,
  GUID_REGISTRO Varchar2(100 ),
  ES_ULTIMA_SEDE Number(1,0),
  ACTIVO Number(1,0),
  ID_CREADOR Number,
  FECHA_CREACION Date
)
/

-- Add keys for table SHM_SEG_USUARIO_SEDE

ALTER TABLE SHM_SEG_USUARIO_SEDE ADD CONSTRAINT PK_SHM_SEG_USUARIO_SEDE PRIMARY KEY (ID_USUARIO,ID_SEDE)
/


ALTER TABLE SHM_SEG_USUARIO_SEDE ADD CONSTRAINT FK_SEG_USUARIO_SEDE FOREIGN KEY (ID_USUARIO) REFERENCES SHM_SEG_USUARIO (ID_USUARIO)
/

ALTER TABLE SHM_SEG_USUARIO_SEDE ADD CONSTRAINT FK_SEDE_USUARIO FOREIGN KEY (ID_SEDE) REFERENCES SHM_SEDE (ID_SEDE)
/

